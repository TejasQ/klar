  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "typings",
    "types",
    "codegen",
    "backend",
    "data layer"
  ],
  "files": [
    "src"
  ],
  "bin": {
    "klar": "./src/index.js"
  },
  "bugs": {
    "email": "tejas@tejas.qa"
  },
  "author": {
    "name": "Tejas Kumar",
    "email": "tejas@tejas.qa",
