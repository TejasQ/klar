  },
  "contributors": [
    "Tejas Kumar"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tejasq/klar"
  },
  "scripts": {
    "start": "nodemon -w src --exec \"node src/index.js\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
