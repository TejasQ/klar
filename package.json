  "devDependencies": {
    "doctoc": "^1.3.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jest-cli": "^23.4.1",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.5"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "async": "^2.6.1",
    "case": "^1.5.5",
    "is-online": "^7.0.0",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.1.2",
    "ora": "^2.1.0",
    "pkg-dir": "^3.0.0"
  }
